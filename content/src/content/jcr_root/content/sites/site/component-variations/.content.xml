<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:kes="http://kestros.io/kes/1.0"
  jcr:primaryType="kes:Page">
  <jcr:content
    jcr:primaryType="nt:unstructured"
    sling:resourceType="site/components/pages/site-base-page"
    jcr:title="Component Variations">
    <main jcr:primaryType="nt:unstructured">
      <container
        jcr:primaryType="nt:unstructured"
        sling:resourceType="kestros/commons/components/structure/container">
        <content
          jcr:primaryType="nt:unstructured"
          sling:resourceType="kestros/commons/components/content-area">
          <button-plain
            jcr:primaryType="nt:unstructured"
            sling:resourceType="kestros/commons/components/content/button"
            text="Leave me as a plain button"
            link="#"/>
          <button-full-width
            jcr:primaryType="nt:unstructured"
            sling:resourceType="kestros/commons/components/content/button"
            text="Add a full width variation"
            link="#"/>
          <button-background-color
            jcr:primaryType="nt:unstructured"
            sling:resourceType="kestros/commons/components/content/button"
            text="Add a background color variation"
            link="#"/>
          <button-inline-variation
            jcr:primaryType="nt:unstructured"
            sling:resourceType="kestros/commons/components/content/button"
            text="Add an inline variation"
            link="#"/>
          <button-multiple-variations
            jcr:primaryType="nt:unstructured"
            sling:resourceType="kestros/commons/components/content/button"
            text="Add multiple variations"
            link="#"/>
        </content>
      </container>
    </main>
  </jcr:content>
</jcr:root>

